{
  "locals": {
    "url": "http://localhost:8080",
    "name": "Darko's resume",
    "owner": "Someone",
    "description": "My story so far"
  },
  "plugins": [
    "./plugins/paginator.coffee",
    "./node_modules/wintersmith-handlebars/wintersmith-handlebars.coffee"
  ],
  "handlebars": {
    "partialDir": "partials",
    "helperDir": "helpers"
  },
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "template": "index.html",
    "first": "index.html",
    "perPage": 20
  }
}
